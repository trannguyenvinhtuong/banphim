<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <script src="https://kit.fontawesome.com/2a8f00c7fa.js" crossorigin="anonymous"></script>
    <link type="text/css" rel="stylesheet" href="@Url.Content("~/Content/css/Site.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.Content("~/Content/css/style.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.Content("~/Content/css/aboutstyle.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.Content("~/Content/css/login.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.Content("~/Content/css/GioHang.css")" />
</head>
<body>
    <div>
        <div class="header-style">
            <nav class="navbar navbar-expand-lg navbar-light sticky-top">
                <a class="navbar-brand" href="#">Keyboard</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="collapsibleNavbar">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item menu-header">
                            <a class="nav-link" href="@Url.Action("Index","Home")">Home</a>
                        </li>
                        <li class="nav-item menu-header">
                            <a class="nav-link " href="@Url.Action("Login","Admin")">Admin</a>
                        </li>
                        <li class="nav-item menu-header">
                            <a class="nav-link" href="#">News</a>
                        </li>
                        <li class="nav-item menu-header">
                            <a class="nav-link" href="@Url.Action("GioHang","GioHang")"><i class="fas fa-shopping-cart">  Giỏ Hàng</i></a>
                        </li>
                        <li class="nav-item menu-header">
                            <a class="nav-link" href="@Url.Action("About","Home")">About</a>
                        </li>
                        <li class="nav-item menu-header" style="margin-right: 40px">
                            <a class="nav-link" href="@Url.Action("DNKH","Home")">Login</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
    </div>
    
        @RenderBody()
        <hr />
        <footer>

            <h1 class="footer-content">Kết nối với chúng tôi <a href="#"><i class="fab fa-facebook"></i></a></h1>
            

            <p>&copy; @DateTime.Now.Year - New Project</p>
        </footer>
  

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


</body>
</html>
